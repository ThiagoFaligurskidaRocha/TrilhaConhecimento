#criar a branch escolhendo o nome
git checkout -o adicionarTelaUsuario
                adicionar_tela_usuario
                AdicionarTelaUsuario

#salvar as alterações da branch
git commit -m "adicionei o campo de cpf na tela do usuário"

#ver as diferenças 
git diff 

#mudar de branch
git checkout -b NOMEDABRANCH

#adicionar arquivo na branch
git add nome_do_arquivo

#remover arquivo na branch
git remove nome_do_arquivo

#ver alterações
git log

#voltar 
